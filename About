      function getCookie(t) {
        for (
          var e = t + "=",
            n = decodeURIComponent(document.cookie).split(";"),
            o = 0;
          o < n.length;
          o++
        ) {
          for (var i = n[o]; " " == i.charAt(0); ) i = i.substring(1);
          if (0 == i.indexOf(e)) return i.substring(e.length, i.length);
        }
        return "";
      }
